#Replace "2_4" with appropriate version number

python alpaca_neural_bot_v2_4.py #Live trading, use cached models
python alpaca_neural_bot_v2_4.py --DEBUG #Live trading, use cached models but with debug mode true 
python alpaca_neural_bot_v2_4.py --force-train #Live trading, retrain models
python alpaca_neural_bot_v2_4.py --force-train --DEBUG #Live trading, retrain models with debug mode true
python alpaca_neural_bot_v2_4.py --backtest #Backtest, use cached models
python alpaca_neural_bot_v2_4.py --backtest --DEBUG #Backtest, use cached models with debug mode true
python alpaca_neural_bot_v2_4.py --backtest --force-train #Backtest, retrain models
python alpaca_neural_bot_v2_4.py --backtest --force-train --DEBUG #Backtest, retrain models with debug mode ture
